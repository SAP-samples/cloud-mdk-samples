{
	"Controls": [
		{
			"_Type": "Control.Type.SectionedTable",
			"_Name": "FormCellContainer",
			"Sections": [
				{
					"Controls": [
						{
							"Value": false,
							"_Type": "Control.Type.FormCell.Switch",
							"_Name": "EnableLogSwitch",
							"IsVisible": true,
							"Separator": true,
							"Caption": "$(L,'enable_logging')",
							"OnValueChange": "/MDKDemoApp/Rules/Logging/ToggleLogging.js",
							"IsEditable": true
						},
						{
							"IsSearchEnabled": false,
							"_Type": "Control.Type.FormCell.ListPicker",
							"_Name": "LogLevelListPicker",
							"IsVisible": true,
							"Separator": true,
							"AllowMultipleSelection": false,
							"AllowEmptySelection": true,
							"Caption": "$(L,'log_level')",
							"OnValueChange": "/MDKDemoApp/Rules/Logging/SetUserLogLevel.js",
							"IsSelectedSectionEnabled": false,
							"IsPickerDismissedOnSelection": true,
							"AllowDefaultValueIfOneItem": false,
							"IsEditable": false,
							"PickerItems": "/MDKDemoApp/Rules/Logging/LogLevels.js"
						}
					],
					"Visible": true,
					"EmptySection": {
						"FooterVisible": false
					},
					"Separators": {
						"TopSectionSeparator": false,
						"BottomSectionSeparator": true,
						"HeaderSeparator": true,
						"FooterSeparator": true,
						"ControlSeparator": true
					},
					"_Type": "Section.Type.FormCell",
					"_Name": "FormCellContainerSectionFormCell0"
				},
				{
					"Controls": [
						{
							"_Type": "Control.Type.FormCell.Button",
							"_Name": "Send",
							"IsVisible": true,
							"Separator": true,
							"Title": "$(L,'send_activity_log')",
							"Alignment": "Center",
							"ButtonType": "Text",
							"Semantic": "Tint",
							"ImagePosition": "Leading",
							"Enabled": true
						}
					],
					"Visible": true,
					"EmptySection": {
						"FooterVisible": false
					},
					"Separators": {
						"TopSectionSeparator": false,
						"BottomSectionSeparator": true,
						"HeaderSeparator": true,
						"FooterSeparator": true,
						"ControlSeparator": true
					},
					"_Type": "Section.Type.FormCell",
					"_Name": "FormCellContainerSectionFormCell1"
				}
			],
			"FilterFeedbackBar": {
				"ShowAllFilters": false,
				"_Type": "Control.Type.FilterFeedbackBar"
			}
		}
	],
	"_Type": "Page",
	"_Name": "UserActivityLog",
	"OnLoaded": "/MDKDemoApp/Rules/Logging/UserLogSetting.js",
	"ActionBar": {
		"Items": [],
		"_Name": "ActionBar1",
		"_Type": "Control.Type.ActionBar",
		"Caption": "$(L,'activity_log_caption')",
		"PrefersLargeCaption": "$(PLT,true,false,false)"
	}
}