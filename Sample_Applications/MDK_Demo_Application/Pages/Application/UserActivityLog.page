{
	"Controls": [
		{
			"_Type": "Control.Type.FormCellContainer",
			"_Name": "FormCellContainer",
			"Sections": [
				{
					"Controls": [
						{
							"Value": false,
							"_Type": "Control.Type.FormCell.Switch",
							"_Name": "EnableLogSwitch",
							"IsEditable": true,
							"Caption": "Enable Logging",
							"OnValueChange": "/MDKDemoApp/Rules/Logging/ToggleLogging.js",
							"Value": "/MDKDemoApp/Rules/Logging/IsLoggerOn.js"
						},
						{
							"IsSearchEnabled": false,
							"_Type": "Control.Type.FormCell.ListPicker",
							"_Name": "LogLevelListPicker",
							"IsEditable": false,
							"AllowMultipleSelection": false,
							"Caption": "Log Level",
							"OnValueChange": "/MDKDemoApp/Rules/Logging/SetUserLogLevel.js",
							"PickerItems": "/MDKDemoApp/Rules/Logging/LogLevels.js",
							"Value": "/MDKDemoApp/Rules/Logging/GetCurrentLogLevel.js"
						},
						{
							"_Type": "Control.Type.FormCell.ListPicker",
							"_Name": "CategoriesListPicker",
							"IsEditable": true,
							"IsVisible": false,
							"AllowMultipleSelection": true,
							"AllowEmptySelection": true,
							"Caption": "Tracing Categories",
							"DataPaging": {
								"ShowLoadingIndicator": false,
								"PageSize": 50
							},
							"PickerPrompt": "Please select one or more categories to enable",
							"OnValueChange": "/MDKDemoApp/Rules/Logging/ToggleCategories.js",
							"IsSelectedSectionEnabled": true,
							"AllowDefaultValueIfOneItem": false,
							"PickerItems": "/MDKDemoApp/Rules/Logging/TracingCategories.js",
							"Value": "/MDKDemoApp/Rules/Logging/DefaultCategoriesList.js"
						}
					],
					"Visible": true
				},
				{
					"Controls": [
						{
							"_Type": "Control.Type.FormCell.Button",
							"_Name": "Send",
							"Title": "Send Activity Log",
							"TextAlignment": "Center",
							"Alignment": "Center",
							"OnPress": "/MDKDemoApp/Actions/Logging/UploadLogProgress.action"
						}
					],
					"Visible": true
				}
			]
		}
	],
	"_Type": "Page",
	"_Name": "UserActivityLog",
	"Caption": "Activity Log",
	"OnLoaded": "/MDKDemoApp/Rules/Logging/UserLogSetting.js"
}