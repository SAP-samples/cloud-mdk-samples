{
	"Controls": [
		{
			"_Type": "Control.Type.SectionedTable",
			"_Name": "SectionedTable0",
			"Sections": [
				{
					"_Type": "Section.Type.ContactCell",
					"Target": {
						"EntitySet": "Customers",
						"Service": "/MDKDemoApp/Services/Sample.service"
					},
					"_Name": "SectionContactCellTable0",
					"Header": {
						"_Type": "SectionCommon.Type.Header",
						"_Name": "SectionCommonTypeHeader0",
						"AccessoryType": "None",
						"UseTopPadding": false
					},
					"Visible": true,
					"EmptySection": {
						"Caption": "$(L,'no_customers')",
						"FooterVisible": false
					},
					"ContactCell": {
						"Visible": true,
						"DetailImage": "/MDKDemoApp/Rules/Customer/GetSmallCustomerAvatarURL.js",
						"Headline": "{FirstName} {LastName}",
						"Subheadline": "{City}, {Country}",
						"OnPress": "/MDKDemoApp/Actions/Customer/NavToCustomer_Detail.action",
						"ActivityItems": [
							{
								"ActivityType": "Phone",
								"ActivityValue": "{PhoneNumber}"
							},
							{
								"ActivityType": "Email",
								"ActivityValue": "{EmailAddress}"
							}
						],
						"ContextMenu": {
							"PerformFirstActionWithFullSwipe": true
						}
					},
					"DataPaging": {
						"LoadingIndicatorText": "",
						"ShowLoadingIndicator": true
					},
					"Search": {
						"Enabled": true,
						"Placeholder": "$(L,'search_customers')"
					}
				}
			],
			"LoadingIndicator": {
				"Enabled": true,
				"Text": ""
			},
			"FilterFeedbackBar": {
				"ShowAllFilters": false,
				"_Type": "Control.Type.FilterFeedbackBar"
			}
		}
	],
	"_Type": "Page",
	"_Name": "Customer_List",
	"ActionBar": {
		"Items": [],
		"_Name": "ActionBar1",
		"_Type": "Control.Type.ActionBar",
		"Caption": "$(L,'customer_list_caption')",
		"PrefersLargeCaption": "$(PLT,true,false,false)"
	}
}