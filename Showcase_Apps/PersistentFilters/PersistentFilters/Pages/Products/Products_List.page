{
	"Controls": [
		{
			"_Type": "Control.Type.SectionedTable",
			"_Name": "SectionedTable",
			"Sections": [
				{
					"Header": {
						"UseTopPadding": false,
						"_Type": "SectionCommon.Type.Header"
					},
					"_Type": "Section.Type.ObjectTable",
					"Target": {
						"Service": "/PersistentFilters/Services/SampleServiceV4.service",
						"EntitySet": "Products",
						"QueryOptions": ""
					},
					"_Name": "SectionObjectTable0",
					"EmptySection": {
						"FooterVisible": false
					},
					"ObjectCell": {
						"ContextMenu": {
							"Items": [],
							"PerformFirstActionWithFullSwipe": true,
							"_Type": "ObjectCell.Type.ContextMenu"
						},
						"Title": "{Name}",
						"Subhead": "{CategoryName}",
						"Footnote": "{DimensionWidth} {DimensionUnit} x {DimensionHeight} {DimensionUnit} x {DimensionDepth} {DimensionUnit}",
						"StatusText": "$(C,{Price},{CurrencyCode},'',{minimumIntegerDigits:1,minimumFractionDigits:0,maximumFractionDigits:2,useGrouping:true})",
						"SubstatusText": "{QuantityUnit}",
						"AvatarStack": {
							"Avatars": [
								{
									"Image": "/PersistentFilters/Services/SampleServiceV4.service/{@odata.readLink}/Picture"
								}
							],
							"ImageIsCircular": false
						},
						"PreserveIconStackSpacing": false,
						"AccessoryType": "DisclosureIndicator",
						"OnPress": "/PersistentFilters/Actions/Products/NavToProducts_Detail.action",
						"_Type": "ObjectTable.Type.ObjectCell"
					},
					"Search": {
						"Enabled": true,
						"Placeholder": "Item Search",
						"BarcodeScanner": true,
						"Delay": 500,
						"MinimumCharacterThreshold": 3
					},
					"DataPaging": {
						"ShowLoadingIndicator": true,
						"LoadingIndicatorText": "Loading more items, please wait..."
					}
				}
			],
			"LoadingIndicator": {
				"Enabled": true,
				"Text": "Loading, please wait..."
			},
			"Filters": "/PersistentFilters/Rules/Products/LoadFilter.js"
		}
	],
	"_Type": "Page",
	"_Name": "Products_List",
	"OnReturning": "/PersistentFilters/Rules/Products/SaveCurrentFilter.js",
	"ActionBar": {
		"Items": [
			{
				"_Name": "ActionBarItem0",
				"Caption": "Filter",
				"Icon": "sap-icon://filter",
				"Position": "Right",
				"IsIconCircular": false,
				"IconText": "Filter",
				"Visible": true,
				"OnPress": "/PersistentFilters/Actions/Products/ProductFilter.action",
				"_Type": "Control.Type.ActionBarItem"
			}
		],
		"_Name": "ActionBar1",
		"Caption": "Products",
		"_Type": "Control.Type.ActionBar"
	}
}